{
  "name": "module-twitch",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "prestart:prod": "tsc -p .",
    "start:prod": "node dist/index.js",
    "prebuild:x64": "tsc -p .",
    "prebuild:arm64": "tsc -p .",
    "build:x64": "npm-run-all build:x64:*",
    "build:arm64": "npm-run-all build:x64:*",
    "build:x64:linux": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/linux-x64/twitch/twitch --targets node16-linux-x64",
    "build:x64:macos": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/macos-x64/twitch/twitch --targets node16-macos-x64",
    "build:x64:win": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/win-x64/twitch/twitch --targets node16-win-x64",
    "build:arm64:linux": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/linux-arm64/twitch/twitch --targets node16-linux-arm64",
    "build:arm64:macos": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/macos-arm64/twitch/twitch --targets node16-macos-arm64",
    "build:arm64:win": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/win-arm64/twitch/twitch --targets node16-win-arm64",
    "lint": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "node_modules/evntcom-js/dist/**/*.js"
    ],
    "assets": [
      "node_modules/es-get-iterator/**/*",
      "node_modules/iterate-value/**/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@twurple/api": "^5.0.1",
    "@twurple/auth": "^5.0.1",
    "@twurple/chat": "^5.0.1",
    "@twurple/pubsub": "^5.0.1",
    "bufferutil": "^4.0.3",
    "dotenv": "^10.0.0",
    "evntcom-js": "https://github.com/EvntBoard/evntcom-js.git",
    "lodash": "^4.17.21",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.7.10",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.3.1",
    "pkg-fetch": "^3.2.2",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.2.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.4.2"
  }
}
